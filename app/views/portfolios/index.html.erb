<div>
  <span class='#dd2c00 deep-orange-text text-accent-4'>
    <h1>Portfolios</h1>
<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Style</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @portfolios.each do |portfolio| %>
      <tr>
        <td><%= portfolio.artist_id %> </td>
        <td><%= portfolio.style %></td>
        <td><%= link_to 'Show Portfolio', portfolio %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</span>
</div>

<%= link_to 'Show Artists', artists_path %> |
<%= link_to 'Home', root_path %> |
